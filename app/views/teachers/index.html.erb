<div id="k-body" class="k-body adminDesk">
  <div class="container">
<table>
  <%- @teachers.each do |teacher| %> 
    <tr>
      <td>
	<div class="left">
	  <%= teacher.first_name %> <%= teacher.last_name%>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= link_to 'Edit teacher', edit_teacher_path(teacher) %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= link_to 'Delete Teacher', teacher_path(teacher), method: :delete%>
	</div>
      </td>
      
    </tr>
  <%- end %>
</table>

<%= link_to 'New teacher',  new_teacher_path %>
</div>
</div>
