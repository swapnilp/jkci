<div id="k-body" class="k-body adminDesk">
  <div class="container">
<div>
  <h1>Students</h1>
</div>
<%= link_to 'New Student', new_student_path, class: 'btn btn-primary' %>
<div>
  <table class="table table-hover studentsTable">
    <thead>
      <tr>
	<th>#</th>
	<th>Name</th>
	<th>Class</th>
	<th>Group</th>
	<th>Mobile</th>
	<th>Parent Mobile</th>
	<th>Actions</th>
      </tr>
    </thead>
    <%= render partial: 'student', locals: {students: @students}%>
    
  </table>
  <div class="paginationDiv">
    <%= render partial: 'pagination', locals: {students: @students} %>
  </div>
</div>

</div>
</div>
