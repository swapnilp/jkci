<div class="col-sm-12">
  <h2><%= @daily_teaching_point.teacher.try(:name) %>
    <span style="font-size:14px;"> 
      (<%= @daily_teaching_point.jkci_class.try(:class_name)%> - <%= @daily_teaching_point.date.to_date %>)
    </span>
  </h2>
  <div class="textLeft">
    <h3>Points</h3>
    <div>
      <%= @daily_teaching_point.points %>
    </div>
  </div>
  <div class="textLeft" style="margin-top:20px;">
    <%= link_to "Student Presenty", "", class: "btn btn-primary" %>
    <%= link_to "Back", daily_teachs_path, class: "btn btn-warning" %>
  </div>
  <div class="col-sm-12 class">
  </div>
</div>
