<div id="k-body"><!-- content wrapper -->
  <div class="container"><!-- container -->
    <div class="row"><!-- row -->
      <div class="k-breadcrumbs col-lg-12 clearfix"><!-- breadcrumbs -->
        <ol class="breadcrumb">
          <li><a href="#">Home</a></li>
        </ol>
      </div><!-- breadcrumbs end -->
    </div><!-- row end -->
    <div class="row no-gutter fullwidth"><!-- row -->
      <div class="col-lg-12 clearfix"><!-- featured posts slider -->
        <div id="carousel-featured" class="carousel slide" data-interval="4000" data-ride="carousel">
          <div class="carousel-inner"><!-- Wrapper for slides -->
            <div class="item active">
              <img src="https://s3-ap-southeast-1.amazonaws.com/jkciphoto/jkProdPhoto/useful/home_page.jpg" alt="JKSAI" />
              <div class="k-carousel-caption pos-1-3-left scheme-light">
                <div class="caption-content">
                  <h3 class="caption-title">Learning Today . . . Leading Tomorrow.</h3>
                </div>
              </div>
            </div>
          </div><!-- Wrapper for slides end -->
        </div><!-- featured posts slider wrapper end -->
      </div><!-- featured posts slider end -->
    </div><!-- row end -->
    <div>
      <h2 style="margin-top:10px">Medical Student Results</h2>
      <div class="marquee-outer">
        <div class="marquee-inner">
          <img src="/assets/medical_strip.jpg" alt="JKSAI" style="max-width:430%"/>
        </div>
      </div>
    </div>
    <div>
      <h2 style="margin-top:10px">Engineering Student Results</h2>
      <div class="marquee-outer">
        <div class="marquee-inner">
          <img src="/assets/eng_strip.jpg" alt="JKSAI" style="max-width:430%"/>
        </div>
      </div>
    </div>
    <div class="row no-gutter"><!-- row -->
      <%= render partial: "events" %>
      <%= render partial: "director_desk" %>
      <%= render partial: "results_desk" %>
    </div><!-- row end -->
  </div><!-- container end -->
</div><!-- content wrapper end -->

