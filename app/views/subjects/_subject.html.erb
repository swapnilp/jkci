<%- subjects.each_with_index do |subject, index| %>
  <tr>
    <td>
      <%= (((params[:page] || 1).to_i - 1) * 10) + index + 1 %>
    </td>
    <td>
      <%= subject.name %>
    </td>
    <td>
      <%= link_to 'Show', subject_path(subject) %>  | 
      <%= link_to 'Edit', edit_subject_path(subject) %>
    </td>
  </tr>
<%- end %>

