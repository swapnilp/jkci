<div style="width:100%;">
  <table width="100%;">
    <thead>
      <tr>
	<th>#</th>
	<th>Name</th>
	<th>Start Time</th>
	<th>End Time</th>
	<th>Teacher</th>
	<th>Show</th>
	<th>Edit</th>
	<th>Delete</th>
      </tr>
      </thead>
    <%- @jkci_classes.each do |jkci_class| %> 
    <tr>
      <td>
	<div class="left">
	  <%= jkci_class.id %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= jkci_class.class_name %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= jkci_class.class_start_time %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= jkci_class.class_end_time %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= jkci_class.teacher.try(:name) %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= link_to 'show', jkci_class_path(jkci_class) %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= link_to 'Edit', edit_jkci_class_path(jkci_class) %>
	</div>
      </td>
      <td>
	<div class="left">
	  <%= link_to 'Delete', jkci_class_path(jkci_class), method: :delete%>
	</div>
      </td>
      
    </tr>
    <%- end %>
  </table>
</div>
<div style="margin-top:20px;">
  <%= link_to 'New class',  new_jkci_class_path, class: 'btn btn-primary btn-sm' %>
</div>

