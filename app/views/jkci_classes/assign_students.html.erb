<h1>
  <%= @jkci_class.class_name %>
</h1>

<div>
  <h2>Sasd sad</h2>
</div>
<div class="left assignedStudents">
  <%- @students.each do |student| %>
    <div style="text-align:left;">
      <%= check_box_tag student.id, student.name, @selected_students.include?(student.id) %> 
      <label for="<%= student.id%>" style="display:inline-block;"><%= "#{student.name} - #{student.group}  - #{student.std}" %> </labbel>
    </div>
  <%- end %>
</div>

<div class="left" style="width:100%;margin-top:20px;">
  <%= link_to 'Submit', "#", onclick: "assignClassStudents(event, this);", class: 'btn btn-primary left', data: {id: @jkci_class.id}  %>
</div>

