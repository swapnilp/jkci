<div id="k-body"><!-- content wrapper -->
  <div class="container"><!-- container -->
    <div class="row"><!-- row -->
      <div id="k-top-search" class="col-lg-12 clearfix"><!-- top search -->
        <form action="#" id="top-searchform" method="get" role="search">
          <div class="input-group">
            <input type="text" name="s" id="sitesearch" class="form-control" autocomplete="off" placeholder="Type in keyword(s) then hit Enter on keyboard">
          </div>
        </form>
        <div id="bt-toggle-search" class="search-icon text-center">
	  <i class="s-open fa fa-search"></i><i class="s-close fa fa-times"></i>
	</div><!-- toggle search button -->
      </div><!-- top search end -->
      <div class="k-breadcrumbs col-lg-12 clearfix"><!-- breadcrumbs -->
        <ol class="breadcrumb">
          <li><a href="<%= root_path %>">Home</a></li>
          <li class="active">Results</li>
        </ol>
      </div><!-- breadcrumbs end -->               
    </div><!-- row end -->
    <div class="row no-gutter"><!-- row -->
      <div class="col-lg-8 col-md-8"><!-- doc body wrapper -->
        <div class="col-padded"><!-- inner custom column -->
          <div class="row gutter"><!-- row -->
            <div class="col-lg-12 col-md-12">
              <h1 class="page-title">Results</h1><!-- category title -->
              <div class="category-description"><!-- category description -->
                <p>
		  J K Science Academy has a great history of
		  successful results since 25 years. We have credit to
		  produce students securing marks from 90 to 100 in
		  different subjects. Our results add a new shining
		  feather in our history. We are proud to have a great
		  history of results.
                </p>
              </div>
            </div>
          </div><!-- row end -->
          <div class="row gutter k-equal-height"><!-- row -->
	    <%- @results.each do |result| %>
              <div class="news-mini-wrap col-lg-6 col-md-6 batchResultDiv"><!-- news mini-wrap -->
		<figure class="news-featured-image">
		  <a href="<%= result_path(result) %>" title="<%= result.title %>">
		    <img src="<%= result.cover_img %>" alt="<%= result.title %>" class="img-responsive">
		  </a>
		</figure>
		<div class="page-title-meta">
                  <h1 class="page-title">
		    <a href="#" title="Cody Rotschild enjoys...">
		      <%= result.title %>
		    </a>
		  </h1>
                  <div class="news-meta">
                    <span class="news-meta-date red">
		      <a href="<%= result_path(result) %>"><%= result.batch %></a>
		    </span>
                    <span class="news-meta-comments">
		      <%= result.published_results.count %>
		    </span>
                  </div>
		</div>
		<div class="news-summary">
                  <p>
		    <%= result.description.truncate(100) %>
                  </p>
		</div>
              </div><!-- news mini-wrap end -->
	    <%- end %>
          </div><!-- row end -->
          <div class="row gutter"><!-- row -->
            <div class="col-lg-12">
	      <%= paginate @results, theme: 'default_pagination' %>
            </div>
          </div><!-- row end -->
        </div><!-- inner custom column end -->
      </div><!-- doc body wrapper end -->
      <%= render partial: 'layouts/side_menu' %>
    </div><!-- row end -->
  </div><!-- container end -->
</div>
